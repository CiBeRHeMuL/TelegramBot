{
    "name": "andrew-gos/telegram-bot",
    "description": "Telegram bot library",
    "type": "library",
    "autoload": {
        "psr-4": {
            "AndrewGos\\TelegramBot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "a.gostev",
            "email": "Gostev71@outlook.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2",
        "ext-curl": ">=8",
        "psr/http-message": ">=1.0.1",
        "psr/http-client": ">=1.0.3",
        "psr/http-factory": ">=1.0.2",
        "psr/http-server-handler": ">=1.0.2",
        "psr/http-server-middleware": ">=1.0.2",
        "psr/container": ">=2.0.2",
        "ext-ctype": "*",
        "psr/log": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=11.0.3",
        "php-query/php-query": "@dev"
    }
}
